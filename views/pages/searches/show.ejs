<!DOCTYPE html>
<html>
    <%- include('../layout/head') %>
<body>
    <%- include('../layout/header') %>
  <main>
    <h1>Search Results</h1>

    <ul>
      <% searchResults.forEach(book => { %>
        <li>
          <img src=<%= book.image_url %> alt=Cover for <%= book.title %> />
          <h2><%= book.title %></h2>
          <h3>by <%= book.author %></h3>
          <p><%= book.description %></p>
        </li>
      <% }) %>
  </main>
</body>
</html>